                    xmlns:viewModels="clr-namespace:GM.ViewModel"
                    xmlns:view="clr-namespace:GM.View">
    <DataTemplate DataType="{x:Type viewModels:TeamsOverviewViewModel}">
        <view:TeamOverviewControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:AllSkatersOverviewViewModel}">
        <view:SkaterOverviewControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:AllGoaliesOverviewViewModel}">
        <view:GoaliesOverviewControl/>
    </DataTemplate>

</ResourceDictionary>